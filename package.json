{
  "name": "reactapp",
  "version": "1.0.0",
  "description": "react app",
  "main": "server/index.js",
  "scripts": {
    "test": "jest -u",
    "start": "cross-env NODE_ENV=development node ./server",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config",
    "clean": "rimraf dist/",
    "prod": "cross-env NODE_ENV=production node ./server",
    "cypress:open": "cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OMManko/React.git"
  },
  "author": "OManko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/OMManko/React/issues"
  },
  "homepage": "https://github.com/OMManko/React#readme",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.1.0",
    "node-sass": "^7.0.0",
    "prop-types": "latest",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-hot-loader": "^4.7.2",
    "react-redux": "^6.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^5.0.0",
    "react-test-renderer": "^16.8.4",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.0",
    "cypress": "^3.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "eslint-plugin-cypress": "^2.2.1",
    "express": "^4.16.4",
    "fetch-mock": "^7.3.1",
    "html-webpack-plugin": "^3.2.0",
    "jest-enzyme": "^7.0.2",
    "mini-css-extract-plugin": "^0.5.0",
    "react-native": "^0.59.5",
    "react-router-native": "^5.0.0",
    "redux-devtools": "^3.5.0",
    "redux-mock-store": "^1.5.3",
    "redux-persist": "^5.10.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-hot-server-middleware": "^0.5.0",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.7.2",
    "webpack-dev-server": "3.2.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "modulePaths": [
      "<rootDir>/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/config/styleMocks.js",
      "\\.(png|jpg|gif|ttf|eot|svg)$": "<rootDir>/config/fileMocks.js"
    },
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/cypress/"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!test-component).+\\.js$",
      "/cypress/"
    ]
  },
  "cypress": {
    "baseUrl": "http://localhost:3000"
  }
}
